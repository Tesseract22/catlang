
proc main() {

    let arr := [1, 2, 3, 4];

    let i := 0;
    loop i < arr.len {
        print(arr[i]);
        i = i + 1;
    }

    print(foo()[0] + foo()[1]);

    let matrix := [[1,2],[3,4]];
    let row := 0;
    let col := 0;

    loop row < matrix.len {
        loop col < matrix[row].len {
            print(matrix[row][col]);
        }
    }
}



fn foo(): [2]int {
    let x := 50;
    let y := 100;
    ret [x, y];
}