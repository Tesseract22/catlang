
proc main() {

    let arr := [1, 2, 3, 4];

    let i := 0;
    loop i < arr.len {
        print(arr[i]);
        i = i + 1;
    }


    let matrix := [[5,6,7],[8,9,10]];


    let row := 0;
    loop row < matrix.len {
        let col := 0;
        loop col < matrix[row].len {
            print(matrix[row][col]);
            col = col + 1;
        }
        row = row + 1;
    }

    print(foo()[0] + foo()[1]);

}



fn foo(): [2]int {
    let x := 50;
    let y := 100;
    ret [x, y];
}