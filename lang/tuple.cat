proc main() {
    let tuple := {1, 3.14, "hello world", {2, [1, 2, 3]}};
    foo(tuple);
}


proc foo(tuple: {int, float, *char, {int, [3]int}}) {
    print(tuple[0]);
    print(tuple[1]);
    print(tuple[2]);
    print(tuple[3][0]);

    print(tuple[3][1][0]);
    print(tuple[3][1][1]);
    print(tuple[3][1][2]);
}